package Shape;

public class Circle implements IShape{
    double radsius;

    public Circle(double radsius) {
        this.radsius = radsius;
    }

    @Override
    public double getArea() {
        return Math.PI * this.radsius *this.radsius;
    }

    @Override
    public double getPerimeter() {
        return 2*Math.PI*this.radsius;
    }
}
